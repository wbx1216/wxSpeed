  height: 80rpx;
}
.bottom{ 
  top:auto;
  margin-top:80rpx;
  animation: bottomAn 0.3s  linear forwards; 
}
@keyframes bottomAn {
  0%{
    bottom:450rpx; 
    width: 550rpx; 
  }
  100%{
    bottom:80rpx; 
    width: 650rpx; 
  }
}
.tool .item {
  display: flex;
  align-items: center;
  font-size: 22rpx;
}

.tool .item 	image {
  display: block;
  width: 56rpx;
  height: 56rpx;
}

.tool .item	.text {
  font-size: 28rpx;
  color: #fff;
  margin-left: 7px;
}

.tool .loading {
  width: 15%;
  margin: auto 5px;
  height: 7px;
  border-radius: 20px;
  position: absolute;
  top: 0;
  bottom: 0;
}

.tool .down {
  animation: right 1s ease infinite;
  background: linear-gradient(to right, transparent, rgb(105, 227, 194));
  transform: rotate(180deg);
  right: 0;
}

.tool .up {
  animation: left 1s ease infinite;
  background: linear-gradient(to right, transparent, #00dbff);
  left: 0;
}

@keyframes right {
  0% {
    opacity: 1;
    width: 10%;
  }

  70% {
    width: 80%;
    right: 10%;
  }

  100% {
    opacity: 0;
  }
}
@keyframes left {
  0% {
    opacity: 1;
    width: 10%;
  }

  70% {
    width: 80%;
    left: 10%;
  }

  100% {
    opacity: 0;
  }
}

