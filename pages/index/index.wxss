  }
}
.cir_out {
  border: 1px solid;
  border-color: #50e3c2;
  width: 200px;
  height: 200px;
  border-radius: 50%;
  margin: auto;
  position: absolute;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  animation: startCircleAni 2.8s ease-in 1.5s infinite;
  -webkit-animation: startCircleAni 2.8s ease-in 1.5s infinite;
  opacity: 0;
  /* Safari å’Œ Chrome */
}

.cir_in {
  width: 177px;
  height: 177px;
  border-radius: 50%;
  position: absolute;
  left: 0rem;
  top: 0rem;
  bottom: 0;
  right: 0;
  margin: auto;
  color: #fff;
  font-size: 50rpx;
  text-align: center;
  line-height: 177px;
  animation: inmove 2.8s ease-in 0.5s infinite;
  -webkit-animation: inmove 2.8s ease-in 0.5s infinite;
  background-image: linear-gradient(to bottom right, #50e3c2, #24cae4);
}

@keyframes inmove {
  20% {
    -webkit-transform: scale(0.85);
    transform: scale(0.85);
  }

  30% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }

  to {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

@keyframes startCircleAni {
  20% {
    opacity: 1;
  }

  40% {
    -webkit-transform: scale(1.19);
    transform: scale(1.19);
    opacity: 0;
  }

  to {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

.tool {
  width: 550rpx; 
  display: flex;
  margin:0rpx auto;
  left:0;right: 0;
  justify-content: space-between;
